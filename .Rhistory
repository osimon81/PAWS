raw <- read.csv("/Volumes/Simon/LH x CP Experiments/Experiment 1 Data (LH)/LH (PAWS)/PAWS-Analysis/results/PAWS_Results.csv")
raw$body_part <- factor(raw$body_part, levels = c("heel", "center", "toe"))
# plot pre-peak pain score
process <- raw %>%
group_by(group, stimulus, body_part) %>%
summarize(mean = mean(pre.pain_score),
sem = sd(pre.pain_score)/sqrt(n()))
process$stimulus <- factor(process$stimulus, levels = c("CB", "DB", "LP", "HP"))
ggplot(process) +
geom_col(mapping = aes(x = stimulus, y = mean, fill = stimulus), color = "black") +
geom_hline(aes(yintercept = 0)) +
